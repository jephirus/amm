<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="cn.jxust.device.model.Prober" table="prober">
		
		<id name="proberId" type="java.lang.Integer" column="prober_id" unsaved-value="0">
			<generator class="identity"></generator>
		</id>

		<property name="proberNum" type="java.lang.String">
			<column name="prober_num" length="80" />
		</property>
		
		<property name="location" type="java.lang.String">
			<column name="location" length="180" />
		</property>
		
		<property name="proberRange" type="java.lang.String">
			<column name="prober_range" length="20" />
		</property>
		
		<property name="lowThickness" type="java.lang.String">
			<column name="low_thickness" length="10" />
		</property>
		
		<property name="highThickness" type="java.lang.String">
			<column name="high_thickness" length="80" />
		</property>
		
		<property name="installDate" type="java.lang.String">
			<column name="install_date" length="20" />
		</property>
		
        <many-to-one name="device" column="device_id" 
            class="cn.jxust.device.model.Device">   <!-- column="id"外键名  -->  
        </many-to-one>

		<property name="alarmValue" type="java.lang.String">
			<column name="alarm_value" length="40" />
		</property>
		
		<property name="currentStatus" type="java.lang.String">
			<column name="current_status" length="140" />
		</property>
		
		<property name="alarmTime" type="java.lang.String">
			<column name="alarm_time" length="120" />
		</property>
		
		<property name="currentThickness" type="java.lang.String">
			<column name="current_thickness" length="40" />
		</property>
		
		<property name="faultFlag" type="java.lang.Integer">
			<column name="fault_flag" length="4" />
		</property>
		
		<property name="alarmFlag" type="java.lang.Integer">
			<column name="alarm_flag" length="4" />
		</property>
		
	</class>
</hibernate-mapping>
