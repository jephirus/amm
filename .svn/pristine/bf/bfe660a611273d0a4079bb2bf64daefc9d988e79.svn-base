<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.jxust.sms.model.SMS" table="short_message">
		<id name="id" type="java.lang.Integer" column="id" unsaved-value="0">
			<generator class="increment"></generator>
		</id>

		<property name="phonoNumbers" type="java.lang.String">
			<column name="phonoNumbers"></column>
		</property>
		
		<property name="deviceId" type="java.lang.String">
			<column name="deviceId"></column>
		</property>

		<property column="content" name="content"
			type="java.lang.String">
		</property>
	</class>
</hibernate-mapping>
