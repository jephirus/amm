package cn.jxust.sms.model;

/**
 * @author lt
 * @crateTime 2015-7-12 下午07:13:18 
 * 该类封装了发送信息的记录  
 */
public class SMSInfo {
	public static final int SEND_SUCCESS = 1;
	public static final int SEND_FAIL = 0;
	/**
	 * 发送成功的记录,0:表示发送失败，1:表示发送成功
	 */
	public int status;
	/**
	 * 剩余多少钱，单位元
	 */
	public float surplusMoney;
	/**
	 * 剩余多少条短信
	 */
	public int surplusNumber; 
	/**
	 * 短信发送时间 格式：YYYY-MM-dd HH:mm:ss
	 */
	public String postime;
}
